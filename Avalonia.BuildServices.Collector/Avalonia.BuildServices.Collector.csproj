<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <DefineConstants>$(DefineConstants);AVALONIAUI_COLLECTOR</DefineConstants>
        <!-- Target olders runtime -->
        <!-- We also need "rollForward": "Major", defined in runtimeconfig.json in this repo -->
        <CheckEolTargetFramework>false</CheckEolTargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="../BuildTask/Common.cs"/>
        <Compile Include="../BuildTask/Logger.cs"/>
        <Compile Include="../BuildTask/TelemetryPayload.cs"/>
        <Compile Include="../BuildTask/ContinuousIntegrationHelper.cs"/>
    </ItemGroup>
</Project>
